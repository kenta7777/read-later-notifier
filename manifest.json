{
    "name": "read-later-notifier",
    "manifest_version": 2,
    "version": "1.0",
    "browser_action": {
      "default_icon": "my-image.png",
      "default_popup": "popup.html"
    },
    "background" : {
      "scripts" : ["background.js"]
    },
    "content_scripts": [
      {
        "matches": ["https://www.google.com/", "https://www.google.com/"],
        "js": ["content_scripts.js"]
      }
    ],
    "permissions": [
        "bookmarks",
        "alarms",
        "notifications",
        "storage"
    ]
  }
  